{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "lib": ["esnext", "dom", "dom.iterable"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": ["**/*.ts"]
}
